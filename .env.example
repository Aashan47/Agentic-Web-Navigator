# Google Generative AI API Key (required for LLM)
LLM_GOOGLE_API_KEY=your-api-key-here

# LLM Configuration
LLM_MODEL_NAME=gemini-1.5-pro
LLM_TEMPERATURE=0.1
LLM_MAX_OUTPUT_TOKENS=4096

# Browser Configuration
BROWSER_HEADLESS=false
BROWSER_VIEWPORT_WIDTH=1280
BROWSER_VIEWPORT_HEIGHT=720
BROWSER_TIMEOUT_MS=30000
BROWSER_SLOW_MO=100

# DOM Processor Configuration
DOM_MAX_ELEMENTS=500
DOM_MAX_TEXT_LENGTH=100

# Agent Configuration
AGENT_MAX_STEPS=50
AGENT_MAX_RETRIES=3
AGENT_SCREENSHOT_QUALITY=80

# Logging Configuration
LOG_LEVEL=INFO
LOG_LOG_TO_FILE=true
